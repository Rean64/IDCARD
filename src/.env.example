# Frontend Environment Variables
# MINDEF ID-CARD Management System

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_APP_NAME="MINDEF ID-CARD System"
VITE_APP_VERSION=1.0.0

# Authentication
VITE_JWT_SECRET=your_jwt_secret_key_here
VITE_SESSION_TIMEOUT=3600000

# File Upload Configuration
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf

# System Configuration
VITE_APP_ENV=development
VITE_DEBUG_MODE=true
VITE_ENABLE_ANALYTICS=false

# Security Headers
VITE_CSP_ENABLED=true
VITE_SECURE_COOKIES=true

# MINDEF Specific
VITE_ORGANIZATION_NAME=Ministry of Defense
VITE_ORGANIZATION_CODE=MINDEF
VITE_COUNTRY=Cameroon
VITE_SUPPORT_EMAIL=support@mindef.gov.cm
VITE_ADMIN_EMAIL=admin@mindef.gov.cm

# Feature Flags
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_DOCUMENT_UPLOAD=true
VITE_ENABLE_EMAIL_NOTIFICATIONS=false
VITE_ENABLE_SMS_NOTIFICATIONS=false